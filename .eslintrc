{
  "extends":[
    "eslint:recommended",
    "plugin:import/errors",
    "plugin:import/warnings"
  ],
  "globals":{
    "document": false,
    "escape": false,
    "navigator": false,
    "unescape": false,
    "window": false,
    "describe": true,
    "before": true,
    "after":true,
    "it": true,
    "expect": true,
  },
  "env":{
    "browser":false,
    "node":true,
    "mocha":true,
    "es6":true
  },
  "parser": "babel-eslint",
  "parserOptions":{
    "ecmaVersion":6,
    "sourceType":"module",
    "ecmaFeatures":{
      "experimentalObjectRestSpread": true,
      "spread": true
    }
  },
  "rules":{
    "quotes": 0,
    "no-console":1,
    "no-debugger":1,
    "semi": [1,"always"],
    "no-trailing-spaces": 0,
    "eol-last":0,
    "no-unused-vars":0,
    "no-underscore-dangle":0,
    "no-alert":0,
    "no-lone-blocks":0,
    "import/no-unresolved":[2, {"ignore":["^[~]"]}]
  }
}
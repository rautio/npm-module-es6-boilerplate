{
  "name": "npm-module-es6-boilerplate",
  "version": "0.1.0",
  "description": "A boilerplate to build npm modules using es6.",
  "main": "lib/npm-module-es6-boilerplate.min.js",
  "scripts": {
    "test": "jest",
    "coveralls": "jest --coverage --coverageReporters=text-lcov | coveralls",
    "lint": "esw src test",
    "lint:watch": "esw -w src test",
    "clean": "rimraf lib",
    "build": "webpack -p --env build",
    "dev": "webpack --progress --colors --watch --env dev",
    "doc": "jsdoc src -r -d doc/ --private --readme README.md"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rautio/npm-module-es6-boilerplate.git"
  },
  "keywords": [
    "npm",
    "module",
    "boilerplate",
    "es6"
  ],
  "author": {
    "name": "Oskari Rautiainen",
    "email": "oskari.rautiainen@gmail.com"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rautio/npm-module-es6-boilerplate/issues"
  },
  "homepage": "https://github.com/rautio/npm-module-es6-boilerplate#readme",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-loader": "^7.1.2",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-es2015": "^6.24.1",
    "babili-webpack-plugin": "^0.1.2",
    "coveralls": "^3.0.0",
    "eslint": "^4.9.0",
    "eslint-config-google": "^0.9.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-watch": "^3.1.3",
    "jest": "^22.4.4",
    "jsdoc": "^3.5.5",
    "rimraf": "^2.6.2",
    "webpack": "^3.8.1",
    "yargs": "^9.0.1"
  }
}
